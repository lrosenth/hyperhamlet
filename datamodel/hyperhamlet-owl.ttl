<http://rosenthaler.org/ontologies/ontologies/hyperhamlet> rdf:type owl:Ontology ;
                                                            rdfs:comment "Ontology describing the new HyperHamlet data model." ;
                                                            rdfs:label "HyperHamlet" .

#################################################################
#    Object Properties
#################################################################

###  http://rosenthaler.org/ontologies/ontologies/hyperhamlet#hasListRoot
:hasListRoot rdf:type owl:ObjectProperty ;
             rdfs:domain :ListNode ;
             rdfs:range :ListNode .


###  http://rosenthaler.org/ontologies/ontologies/hyperhamlet#hasParent
:hasParent rdf:type owl:ObjectProperty ;
           rdfs:domain :ListNode ;
           rdfs:range :ListNode .


#################################################################
#    Data properties
#################################################################

###  http://rosenthaler.org/ontologies/ontologies/hyperhamlet#isListRoot
:isListRoot rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:boolean .


###  http://rosenthaler.org/ontologies/ontologies/hyperhamlet#sortorder
:sortorder rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:nonNegativeInteger .


#################################################################
#    Classes
#################################################################

###  http://rosenthaler.org/ontologies/ontologies/hyperhamlet#ListNode
:ListNode rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasParent ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :ListNode
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :sortorder ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:nonNegativeInteger
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :isListRoot ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:boolean
                          ] .


